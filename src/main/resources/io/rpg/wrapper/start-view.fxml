<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import java.lang.String?>
<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Button onAction="#onBeginClick" mnemonicParsing="false" stylesheets="@../../../css/styles.css" text="BEGIN">
         <styleClass>
            <String fx:value="menu_button" />
            <String fx:value="big_button" />
         </styleClass>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin></Button>
      <Button onAction="#onExitClick" layoutX="35.0" layoutY="30.0" mnemonicParsing="false" stylesheets="@../../../css/styles.css" text="EXIT">
         <styleClass>
            <String fx:value="menu_button" />
            <String fx:value="big_button" />
         </styleClass>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
